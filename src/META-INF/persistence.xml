<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="exceedvote" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.EmbeddedDriver"></property>
			<property name="javax.persistence.jdbc.url"
				value="jdbc:derby:database.db;create=true"></property>
			<property name="javax.persistence.jdbc.user" value=""></property>
			<property name="eclipselink.ddl-generation" value="create-tables"></property>
			<property name="eclipselink.ddl-generation.output-mode" value="database"></property>
		</properties>
	</persistence-unit>
</persistence>
